<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Session1</servlet-name>
        <servlet-class>Session1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lib_Login</servlet-name>
        <servlet-class>lib_Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lib_SignUp</servlet-name>
        <servlet-class>lib_SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lib_addBook</servlet-name>
        <servlet-class>lib_addBook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Session1</servlet-name>
        <url-pattern>/Session1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>lib_Login</servlet-name>
        <url-pattern>/lib_Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>lib_SignUp</servlet-name>
        <url-pattern>/lib_SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>lib_addBook</servlet-name>
        <url-pattern>/lib_addBook</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
